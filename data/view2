<script language="javascript">
    
    var urlPrefix = "http://10.252.150.157/nguniteview/layoutAction.do?method=showView&ownerType=1&viewId=";    
    var panel = new ViewPanel(TB_2, null,fillJsName, skinJsName, urlPrefix);
    
    var vjs = '{"views":[]}';
    panel.autoBuildMoveView(toJson(vjs), false);
    
    var TD_TOOLBAR = TB_2_R1C2;    
    function toolbarShow(imgobj)
    {
        var display = TD_TOOLBAR.style.display;        
        var src =  imgobj.src;  
        if(display == "none")
        {
            src = src.replace("_open","_close");
            TD_TOOLBAR.style.display = "";
        }
        else
        {
           src = src.replace("_close","_open");  
           TD_TOOLBAR.style.display = "none";   
        }
        imgobj.src = src;
    }
    
    function keepSession(){
    	$.ajax({
		   type: "POST",
		   url: "\layoutAction.do",
		   data: "method=keepSession",
		   success: function(msg){
	   		}
	});
    }
    $(function(){
    	setInterval("keepSession()",1200000);
    });
    
</script>

          
                        
